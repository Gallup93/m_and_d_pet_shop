<h1>Add A New Review</h1>

<%= form_tag "/shelters/#{@shelter.id}/reviews/create" do %>
    <%= label_tag :title %>
    <%= text_field_tag :tile %><br>
    <%= label_tag :rating %>
    <%= text_field_tag :rating %><br>
    <%= label_tag :content %>
    <%= text_field_tag :content %><br>
    <%= label_tag :image %>
    <%= text_field_tag :image %><br>
    <%= submit_tag "Submit Review" %>
<% end %>
